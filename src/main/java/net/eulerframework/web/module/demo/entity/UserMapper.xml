<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.eulerframework.web.module.demo.entity.UserMapper">
  <select id="selectUser" resultType="net.eulerframework.web.module.demo.entity.User">
    select * from SYS_USER where USERNAME = #{username}
  </select>
</mapper>