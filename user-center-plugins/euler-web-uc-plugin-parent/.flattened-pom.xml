<?xml version="1.0" encoding="utf-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.eulerframework.uc.plugins</groupId>
  <artifactId>euler-web-uc-plugin-parent</artifactId>
  <version>1.0.0.M1-SNAPSHOT</version>
  <packaging>pom</packaging>
  <description>Parent POM for all Euler projects.</description>
  <url>https://eulerproject.io/user-center-plugins/euler-web-uc-plugin-parent</url>
  <organization>
    <name>Euler Project</name>
    <url>https://eulerproject.io</url>
  </organization>
  <licenses>
    <license>
      <name>Apache License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <developers>
    <developer>
      <name>cFrost Sun</name>
      <email>cfrost@eulerproject.io</email>
      <organization>Euler Project</organization>
      <organizationUrl>https://eulerproject.io</organizationUrl>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:https://github.com/euler-projects/euler-parent.git/user-center-plugins/euler-web-uc-plugin-parent</connection>
    <developerConnection>scm:git:git@github.com:euler-projects/euler-parent.git/user-center-plugins/euler-web-uc-plugin-parent</developerConnection>
    <url>https://github.com/euler-projects/euler-parent/user-center-plugins/euler-web-uc-plugin-parent</url>
  </scm>
  <properties>
    <build-helper-maven-plugin.version>3.0.0</build-helper-maven-plugin.version>
    <euler-framework.verison>1.0.0.M7-SNAPSHOT</euler-framework.verison>
    <flatten-maven-plugin.version>1.1.0</flatten-maven-plugin.version>
    <servlet-api.verison>3.0.1</servlet-api.verison>
    <xml-maven-plugin.version>1.0.2</xml-maven-plugin.version>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>javax.servlet</groupId>
        <artifactId>javax.servlet-api</artifactId>
        <version>${servlet-api.verison}</version>
      </dependency>
      <dependency>
        <groupId>org.eulerframework</groupId>
        <artifactId>euler-web-core</artifactId>
        <version>${euler-framework.verison}</version>
      </dependency>
      <dependency>
        <groupId>org.eulerframework</groupId>
        <artifactId>euler-web-authentication</artifactId>
        <version>${euler-framework.verison}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>
  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.codehaus.mojo</groupId>
          <artifactId>flatten-maven-plugin</artifactId>
          <version>${flatten-maven-plugin.version}</version>
        </plugin>
        <plugin>
          <groupId>org.codehaus.mojo</groupId>
          <artifactId>xml-maven-plugin</artifactId>
          <version>${xml-maven-plugin.version}</version>
        </plugin>
        <plugin>
          <groupId>org.codehaus.mojo</groupId>
          <artifactId>build-helper-maven-plugin</artifactId>
          <version>${build-helper-maven-plugin.version}</version>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>
</project>
